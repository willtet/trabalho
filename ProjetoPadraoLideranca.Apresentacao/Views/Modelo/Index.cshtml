                    <div class="panel-body">
                        @*Somente números*@
                        <div class="col-lg-2">
                            <div class="form-group">
                                <label class="control-label" for="numeros">Numeros > 0</label>
                                <input type="text" class="texto form-control input-sm" id="numeros" name="sonumero" />
                            </div>
                            <script type="text/javascript">
                                $("#numeros").maskMoney({
                                    prefix: '',
                                    thousands: '',
                                    decimal: '',
                                    precision: 0,
                                    allowZero: false,
                                    allowNegative: false,
                                });
                            </script>
                        </div>
                        <div class="col-lg-2">
                            <div class="form-group">
                                <label class="control-label" for="numerosigual">Numeros >= 0</label>
                                <input type="text" class="texto form-control input-sm" id="numerosigual" name="numerosigual" />
                            </div>
                            <script type="text/javascript">
                                $("#numerosigual").maskMoney({
                                    prefix: '',
                                    thousands: '',
                                    decimal: '',
                                    precision: 0,
                                    allowZero: true,
                                    allowNegative: false,
                                });
                            </script>
                        </div>
                        <div class="col-lg-2">
                            <div class="form-group">
                                <label class="control-label" for="numerosnegativo">Numeros > 0 ou < 0</label>
                                <input type="text" class="texto form-control input-sm" id="numerosnegativo" name="numerosnegativo" />
                            </div>
                            <script type="text/javascript">
                                $("#numerosnegativo").maskMoney({
                                    prefix: '',
                                    thousands: '',
                                    decimal: '',
                                    precision: 0,
                                    allowZero: false,
                                    allowNegative: true,
                                });
                            </script>
                        </div>
                        <div class="col-lg-2">
                            <div class="form-group">
                                <label class="control-label" for="monetario">Numeros Monetario</label>
                                <input type="text" class="texto form-control input-sm" id="monetario" name="monetario" />
                            </div>
                            <script type="text/javascript">
                                $("#monetario").maskMoney({
                                    prefix: 'R$',
                                    thousands: '.',
                                    decimal: ',',
                                    precision: 2,
                                    allowZero: false,
                                    allowNegative: false,
                                });
                            </script>
                        </div>
                        <div class="col-lg-2">
                            <div class="form-group">
                                <label class="control-label" for="decimal">Numeros decimal</label>
                                <input type="text" class="texto form-control input-sm" id="decimal" name="decimal" />
                            </div>
                            <script type="text/javascript">

                                $("#decimal").maskMoney({
                                    prefix: '',
                                    thousands: '.',
                                    decimal: ',',
                                    precision: 2,
                                    allowZero: false,
                                    allowNegative: false,
                                });

                            </script>

                        </div>
                        <div class="col-lg-3">
                            <div class="form-group">
                                <label class="control-label" for="selectList">Exemplo Select com texto</label>
                                <select id="selectList" name="selectList" class="form-control input-sm"></select>
                            </div>
                            <script type="text/javascript">
                                var objArray = [
                                                 { id: 0, nome: 'Objeto 0', otherProp: '321' },
                                                 { id: 1, nome: 'Objeto 1', otherProp: '648' },
                                                 { id: 2, nome: 'Objeto 2', otherProp: '850' },
                                                 { id: 3, nome: 'Objeto 3', otherProp: '046' },
                                                 { id: 4, nome: 'Objeto 4', otherProp: '984' }
                                ];

                                montaSelectList("#selectList", objArray, "id", "nome", "Exemplo Texto Alterado");
                            </script>
                        </div>
                        <div class="col-lg-2">
