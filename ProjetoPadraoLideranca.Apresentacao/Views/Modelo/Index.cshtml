                                                 { id: 2, nome: 'Objeto 2', otherProp: '850' },
                                                 { id: 3, nome: 'Objeto 3', otherProp: '046' },
                                                 { id: 4, nome: 'Objeto 4', otherProp: '984' }
                                ];

                                montaSelectList("#selectList2", objArray, "id", "nome", "");
                            </script>
                        </div>
                        @* Dia Mês Ano datepicker *@
                        <div class="col-lg-2">
