
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="content">
    <div class="row col-xs-12">
        <div class="col-md-12 filtro" style="margin-left:12px; margin-top:5px;">
            <div class="row">
                <div class="panel">
                    <div class="panel-heading">
                        <span class="panel-title"><i class="fa fa-search"></i>Filtrar resultado</span>
                        <span class="panel-title pull-right">
                            <a class="collapse-link">
                                <i class="fa fa-chevron-up"></i>
                            </a>
                        </span>
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-lg-2">
                                <div class="form-group">
                                    <label class="control-label" for="">Projeto</label>
                                    <select id="Projeto" name="Projeto" class="form-control input-sm"></select>
                                </div>
                            </div>
                            <div class="col-lg-4">
                                <div class="form-group">
                                    <label class="control-label" for="">Regime</label>
                                    <input type="text" class="form-control input-sm" id="" name="" />
                                </div>
                            </div>
                            <div class="col-lg-2">
                                <div class="form-group">
                                    <label class="control-label" for="">Mês</label>
                                    <input type="text" class="form-control input-sm" id="" name="" />
                                </div>
                            </div>
                            <div class="col-lg-2">
                                <div class="form-group">
                                    <label class="control-label" for="">Ano</label>
                                    <input type="text" class="form-control input-sm" id="" name="" />
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-12">
                                <div class="form-group" style="float:right">
                                    <label class="control-label" for="btnPesquisar" style="color:white">Pesqu</label>
                                    <a href="javascript:void(0);" class="btn btn-primary btn-sm" id="btnPesquisar" name="btnPesquisar" title="Pesquisar" tabindex="1">
                                        <i class="fa fa-search"></i>&nbsp;Pesquisar
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="panel table-responsive">
                    <div class="panel-heading">
                        <span class="panel-title"><i class="fa fa-list"></i>&nbsp;Consulta</span>
                        <span class="panel-title pull-right">
                            <a class="collapse-link">
                                <i class="fa fa-chevron-up"></i>
                            </a>
                        </span>
                    </div>
                    <div class="panel-body">
                        <table id="tblResultado"
                               data-toggle="table"
                               data-click-to-select="true"
                               data-pagination="true"
                               data-show-footer="false"
                               data-multi-select="true"
                               data-unique-id=""
                               data-page-list="[5, 10, 20, 50, 100, 200, 500, 1000, 10000]"
                               class="table table-hover">
                            <thead>
                                <tr>
                                    <th data-field="" data-align="" data-sortable="true">Projeto</th>
                                    <th data-field="" data-align="" data-sortable="true">Regime</th>
                                    <th data-field="" data-align="" data-sortable="true">Horas lançadas</th>
                                    <th data-field="" data-align="" data-sortable="true">Dias Úteis</th>
                                    <th data-field="" data-align="" data-sortable="true">Horario Util</th>
                                    <th data-field="" data-align="" data-sortable="true">Mês</th>
                                    <th data-field="" data-align="" data-sortable="true">Ano</th>
                                    <th data-field="" data-align="" data-sortable="true">Ass. e Consul. - Pj</th>
                                    <th data-field="" data-align="" data-sortable="true">Salarios e Ordenados</th>
                                    <th data-field="" data-align="" data-sortable="true">Horas Extras Normais</th>
                                    <th data-field="" data-align="" data-sortable="true">Ferias</th>
                                    <th data-field="" data-align="" data-sortable="true">Prov. 13 Salário</th>
                                    <th data-field="" data-align="" data-sortable="true">INSS</th>
                                    <th data-field="" data-align="" data-sortable="true">INSS Prov. Ferias</th>
                                    <th data-field="" data-align="" data-sortable="true">INSS Prov. 13</th>
                                    <th data-field="" data-align="" data-sortable="true">FGTS</th>
                                    <th data-field="" data-align="" data-sortable="true">FGTS Prov. Ferias</th>
                                    <th data-field="" data-align="" data-sortable="true">FGTS Prov. 13</th>
                                    <th data-field="" data-align="" data-sortable="true">Assist. Med.</th>
                                    <th data-field="" data-align="" data-sortable="true">Assist. Odonto.</th>
                                    <th data-field="" data-align="" data-sortable="true">Fundo De Pensão</th>
                                    <th data-field="" data-align="" data-sortable="true">Taxa Adm. Fundo De Pensão</th>
                                    <th data-field="" data-align="" data-sortable="true">Seguro De Vida Em Grupo</th>
                                    <th data-field="" data-align="" data-sortable="true">Vale Transporte</th>
                                    <th data-field="" data-align="" data-sortable="true">Vale Refeição</th>
                                    <th data-field="" data-align="" data-sortable="true">Cesta Básica</th>
                                    <th data-field="" data-align="" data-sortable="true">Auxilio Creche</th>
                                    <th data-field="" data-align="" data-sortable="true">Vale Alimentação</th>
                                    <th data-field="" data-align="" data-sortable="true">Auxilio Baba</th>

                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


@section scripts
{

    <script src="~/Scripts/ScriptsViews/Controle/Index.js?v= @(Guid.NewGuid())"></script>
}